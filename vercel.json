{
    "version": 2,
    "builds": [
      {
        "src": "server.ts",
        "use": "@vercel/node",
        "config": {
          "installCommand": "npm ci",
          "buildCommand": "npm run build",
          "outputDirectory": "dist",
          "includeFiles": ["tsconfig.json", "node_modules"]
        }
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "server.ts"
      }
    ]
  }