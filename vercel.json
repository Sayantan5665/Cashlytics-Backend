{
    "version": 2,
    "builds": [
      {
        "src": "server.ts",
        "use": "@vercel/node",
        "config": {
          "buildCommand": "npm ci && npm i ejs && npm run build",
          "outputDirectory": "dist",
          "includeFiles": ["tsconfig.json"]
        }
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "server.ts"
      }
    ]
  }